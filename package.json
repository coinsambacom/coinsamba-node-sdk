{
  "homepage": "https://github.com/coinsambacom/node-sdk",
  "repository": {
    "type": "git",
    "url": "https://github.com/coinsambacom/node-sdk.git"
  },
  "version": "0.2.1",
  "license": "MIT",
  "private": false,
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "dts watch",
    "build": "dts build",
    "test": "dts test",
    "lint": "dts lint",
    "prepare": "dts build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "dts lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true
  },
  "name": "@coinsamba/node-sdk",
  "author": "Gustavo <gustavo@obex.pw>",
  "module": "dist/node-sdk.esm.js",
  "size-limit": [
    {
      "path": "dist/node-sdk.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/node-sdk.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^8.2.6",
    "@types/got": "^9.6.12",
    "dts-cli": "^2.0.3",
    "husky": "^8.0.3",
    "size-limit": "^8.2.6",
    "tslib": "^2.6.0",
    "typescript": "^5.1.3"
  },
  "dependencies": {
    "axios": "^1.4.0"
  }
}
